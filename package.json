{
  "name": "esbuild-starter",
  "version": "1.0.9",
  "description": "typescript express starter using esbuild",
  "proxy": "http://localhost:3001",
  "main": "index.js",
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/migrate-mongo": "^8.2.0",
    "@types/morgan": "^1.9.3",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/stoppable": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "concurrently": "^7.4.0",
    "craco-esbuild": "^0.5.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.7",
    "esbuild-jest": "^0.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "identity-obj-proxy": "^3.0.0",
    "nodemon": "^2.0.19",
    "react-scripts": "^5.0.1",
    "stoppable": "^1.1.0",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "postinstall": "rm -rf node_modules/saslprep",
    "name": "node name.js",
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development concurrently yarn:dev:*",
    "dev:server-nodemon": "nodemon --enable-source-maps -r dotenv/config dist/index.js dotenv_config_path=.env.local",
    "dev:server-build": "cross-env NODE_ENV=development node esbuild.config.js",
    "dev:ui": "craco start",
    "dev:tsc": "tsc -noEmit --watch --preserveWatchOutput",
    "build": "cross-env NODE_ENV=production concurrently yarn:build:*",
    "build:ui": "cross-env BUILD_PATH=dist/client craco build",
    "build:server": "node esbuild.config.js",
    "postbuild": "cp ecosystem.config.js dist/ecosystem.config.js && cp package.json dist/package.json && cp -r migrations dist/migrations",
    "test": "concurrently test:*",
    "test:acceptance": "jest test/acceptance --runInBand",
    "test:unit": "jest test/unit",
    "test:ui": "craco test --watchAll=false",
    "test:type": "tsc -noEmit",
    "test:lint": "eslint .",
    "lint": "yarn test:lint",
    "jest": "jest",
    "db:createMigration": "migrate-mongo create",
    "postversion": "git push --follow-tags"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "migrate-mongo": "^9.0.0",
    "mongoose": "^6.6.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^3.0.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "nth-check": "^2.0.1"
  }
}
