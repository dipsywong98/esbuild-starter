{
  "name": "ts-express-starter",
  "version": "1.0.0",
  "description": "typescript express starter using esbuild",
  "proxy": "http://localhost:3001",
  "main": "index.js",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/stoppable": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.1.0",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "esbuild-jest": "^0.5.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "nodemon": "^2.0.13",
    "stoppable": "^1.1.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development concurrently yarn:*:watch",
    "build": "cross-env NODE_ENV=production concurrently yarn:*:build",
    "dist:watch": "nodemon dist/index.js",
    "ui:watch": "react-scripts start",
    "type:watch": "tsc -noEmit --watch --preserveWatchOutput",
    "build:watch": "cross-env NODE_ENV=development node esbuild.config.js",
    "ui:build": "react-scripts build",
    "server:build": "node esbuild.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type": "tsc -noEmit",
    "lint": "eslint ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.10",
    "axios": "^0.23.0",
    "esbuild": "^0.13.2",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "web-vitals": "^2.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}